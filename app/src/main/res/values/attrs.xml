    <string-array name="themes">
        <item>Dark</item>
        <item>Light</item>
    </string-array>
    <string name="defaultTheme">Light</string>
    <string name="summaryTheme">Light</string>

    <string-array name="copy">
        <item>Background Always</item>
        <item>Foreground Always</item>
        <item>Ask me Always</item>
    </string-array>
    <string-array name="thumb_size">
        <item>64x64</item>
        <item>128x128</item>
        <item>256x256</item>
        <item>512x512</item>
    </string-array>

