<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="st">
        <attr name="colorPrimary" format="color"/>
        <attr name="colorPrimaryLight" format="color"/>
        <attr name="colorAccent" format="color"/>
        <attr name="lightGrey" format="color"/>
        <attr name="searchHighlight" format="color"/>
        <attr name="transparent" format="color"/>
        <attr name="iconColor1" format="color"/>
        <attr name="iconColor2" format="color"/>
        <attr name="dividerColor" format="color"/>
        <attr name="colorProgress" format="color"/>
        <attr name="textTitle" format="color"/>
        <attr name="normalTextColor" format="color"/>
        <attr name="checkBoxBlank" format="color"/>
        <attr name="background1" format="color"/>
        <attr name="background2" format="color"/>
        <attr name="background3" format="color"/>
