<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center">
        <shape>
            <gradient android:startColor="#315DCA"
                android:centerColor="#55CACA"
                android:angle="90"
                android:endColor="#18485E"/>
            <stroke android:width="1dp"
                android:color="@color/colorPrimary">
            </stroke>
            <solid android:color="#77CFBA"/>
        </shape>

    </item>
    <item android:gravity="bottom">
        <shape>
            <gradient android:startColor="#98A9DC"
                android:centerColor="#B3A3DFE1"
                android:angle="45"
                android:endColor="#B38BB7DA"/>
            <stroke android:width="1dp"
                android:color="#B399C4E6">
            </stroke>
        </shape>
    </item>
</layer-list>


