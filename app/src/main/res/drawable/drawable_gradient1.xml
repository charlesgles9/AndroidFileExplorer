<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item android:gravity="center">
    <shape>
        <gradient android:startColor="#18465E"
            android:centerColor="#752A2A"
            android:angle="180"
            android:endColor="#18485E"/>
        <stroke android:width="1dp"
            android:color="@color/colorPrimary">
        </stroke>
        <solid android:color="#1C5368"/>
    </shape>

    </item>
    <item android:gravity="bottom">
        <shape>
            <gradient android:startColor="#18465E"
                android:centerColor="#81EC4343"
                android:angle="45"
                android:endColor="#18485E"/>
            <stroke android:width="1dp"
                android:color="@color/colorPrimary">
            </stroke>
        </shape>
    </item>
</layer-list>


